get_home_team = function(gid) {
teams = games_raw |>
filter(game_id == gid) |>
pull(team)
loc1 = str_locate(gid, teams[1])[1]
loc2 = str_locate(gid, teams[2])[1]
if (loc1 < loc2) {
return(teams[2])
} else {
return(teams[1])
}
}
get_away_team = function(gid) {
teams = games_raw |>
filter(game_id == gid) |>
pull(team)
loc1 = str_locate(gid, teams[1])[1]
loc2 = str_locate(gid, teams[2])[1]
if (loc1 < loc2) {
return(teams[1])
} else {
return(teams[2])
}
}
